{
    "name": "qms-engine service",
    "serviceName": "qms-engine",
    "env": "dev",
    "host": "0.0.0.0",
    "port": 8085,
    "ownerInfo": {
      "name": "regiewby",
      "email": "regiewby@gmail.com",
      "url": "www.github.com/regiewby"
    },
    "database": {
      "username": "root",
      "password": "",
      "host": "localhost",
      "port": 3306,
      "name": "qms_engine",
      "pool": {
        "idle": 10,
        "max": 100,
        "lifetime": 300
      }
    },
    "redisConfig": {
      "addr": "localhost:6379",
      "idleTimeoutInSec": 60,
      "poolSize": 500,
      "readOnlyFromSlaves": false,
      "readTimeoutInSec": 1,
      "writeTimeoutInSec": 1,
      "tlsEnabled": false
    },
    "statsd": {
      "host": "localhost",
      "port": 8125
    },
    "trace": {
      "host": "localhost",
      "port": 8126,
      "disable": true
    },
    "logger": {
      "workerCount": 10,
      "bufferSize": 10000,
      "logLevel": 2,
      "stacktraceLevel": 4,
      "logFormat": "cglsdebug"
    },
    "kafka": {
      "bootstrap.servers": "localhost:9092",
      "group.id": "qms-engine",
      "auto.offset.reset": "earliest",
      "producer.enabled": true
    }
  }